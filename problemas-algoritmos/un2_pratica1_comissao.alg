algoritmo "comissao"
 var
 matric, tvenda: inteiro
 nome : caractere
 sali, salf, comis : real

 inicio
  Escreva(" Informe o número da matrícula: ")
  leia(matric)
   Se (matric = 12345)entao
    faca
    escreva(" Vendedor Paulo Guina")
     senao
      escreva("[ERRO] Matrícula não encontrada.")
   Fimse

fimalgoritmo
